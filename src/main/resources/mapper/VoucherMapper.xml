<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.sbtest.mapper.VoucherMapper">
    <!--根据id查询用户详情-->
    <select id="getVoucher" parameterType="string" resultType="com.sbtest.dto.Voucher">
        SELECT vou_name as vouName, vou_id as vouId FROM tb_voucher WHERE user_id=#{userId} LIMIT 1;
    </select>
</mapper>
